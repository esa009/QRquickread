{
  "expo": {
    "name": "QR Quick Read",
    "description": "QR Reader",
    "slug": "quickread",
    "privacy": "public",
    "primaryColor": "#34eb8f",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android"],
    "version": "2",
    "orientation": "portrait",
    "icon": "./assets/images/qrlogo.png",
    "android": {
      "versionCode": 2,
      "permissions": ["CAMERA"],
      "package": "com.aesa.qrquickread"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "hooks": {}
  }
}
